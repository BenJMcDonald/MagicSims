package magicGame;

public class CardDB {

	
	
	public static Card getCard(String cardName, Player owner){
		switch(cardName){
		case "Forest":
			return new Card("Forest", "", "LAND", owner);
		case "Grisly Bear":
			return new Card("Grisly Bear", "G1", "CREATURE", owner, 2, 2);
		default: return null;	
		
		}
	}
	
	
	
	
	
	
	
	
	
	
	
}
